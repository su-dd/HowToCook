      },
      {
        "name": "香菜（可选）",
        "quantity": null,
        "unit": null,
        "text_quantity": "- 香菜（可选）",
        "notes": "量未指定"
      },
      {
        "name": "开洋（可选）",
        "quantity": null,
        "unit": null,
        "text_quantity": "- 开洋（可选）",
        "notes": "量未指定"
      },
      {
        "name": "葱",
        "quantity": 80.0,
        "unit": "g",
        "text_quantity": "- 葱 80g",
